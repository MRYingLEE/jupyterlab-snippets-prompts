#!/bin/bash

# install extension
jupyter labextension install jupyterlab-code-snippets

# remove unnecessary directories when using the binder
rm -rf src
rm -rf style
rm -rf Design
rm -rf .github
rm -rf binder
rm package.json
rm tsconfig.json
rm yarn.lock
rm MANIFEST.in

jupyter lab clean

# Import workspace into JupyterLab
jupyter lab workspaces import binder/workspace.json
